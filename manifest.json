{
    "manifest_version": 2,
    "name": "your Photo everywhere",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
         "default_popup": "Popup.html",
         "default_title": "Photo"
    }
}